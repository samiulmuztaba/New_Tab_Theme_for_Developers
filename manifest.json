{
    "manifest_version": 2,
    "name": "Custom New Tab Extension",
    "version": "1.0",
    "description": "Customize your new tab page with this extension.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    
    "browser_action": {
      "default_title": "Custom New Tab"
    }
  }
  